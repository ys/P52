<h1>Listing pictures</h1>

<table>
  <tr>
    <th>Pictureid</th>
    <th>Postdate</th>
    <th>Project</th>
    <th>Description</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pictures.each do |picture| %>
  <tr>
    <td><%= picture.pictureId %></td>
    <td><%= picture.postDate %></td>
    <td><%= picture.project %></td>
    <td><%= picture.description %></td>
    <td><%= picture.title %></td>
    <td><%= link_to 'Show', picture %></td>
    <td><%= link_to 'Edit', edit_picture_path(picture) %></td>
    <td><%= link_to 'Destroy', picture, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Picture', new_picture_path %>
