<% @pictures.each do |picture| %>
	<article class="index_home">
  		<div><%= link_to (image_tag picture.url_m), picture.url, :rel =>'shadowbox[img]' %></div>
		<header>by <%=picture.user.name%></header>
	</article>
<% end %>
</div>
<div class="container project">
<h2>Users: </h2>

<% @users.each do |user| %>
  <%= link_to user.name, user %>  
<% end %>
</div>
<div class="container project">
<h2>Projects</h2>

<% @projects.each do |project| %>
  <%= link_to project.title, user_project_path(project.user,project) %>  
<% end %>



