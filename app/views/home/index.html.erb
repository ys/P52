<h1>Home#index</h1>
<h2>Users: </h2>
<p>
<% @users.each do |user| %>
  <%= link_to user.name, user %>  
<% end %></p>
<h2>Projects</h2>
<p>
<% @projects.each do |project| %>
  <%= link_to project.title, [project.user,project] %>  
<% end %></p>
<h2>Images</h2>

<% @pictures.each do |picture| %>
  <%= link_to (image_tag picture.url_s), [picture.user,picture] %>
<% end %>

